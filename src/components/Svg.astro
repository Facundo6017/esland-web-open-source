---
interface Props {
  src: string;
  width?: number | string;
  height?: number | string;
  class?: string;
  hidden?: boolean;
  label?:string
}

import { resolve } from '@/components/Img/SVG';

const { src, class: className, hidden, label, height, width } = Astro.props;

const content = resolve(src, {
  height,
  width,
  class: className,
  'aria-hidden': hidden,
  'aria-label': label
});

---
<Fragment set:html={content} />

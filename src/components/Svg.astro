---
import type { Props } from '@/components/Img/SVG';
import { resolve } from '@/components/Img/SVG';

const { src, class: className, hidden, label, height, width } = Astro.props;

const content = resolve(src, {
  height,
  width,
  class: className,
  'aria-hidden': hidden,
  'aria-label': label
});

---
<Fragment set:html={content} />
